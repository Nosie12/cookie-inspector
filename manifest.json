{
  "manifest_version": 3,
  "name": "Cookie Insight",
  "version": "1.0",
  "description": "See and understand website cookies without modifying them",
  "icons": {
    "16": "icons/download.jpeg",
    "48": "icons/download (1).jpeg",
    "128": "icons/download (3).png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/download (1).jpeg"
  },
  "permissions": ["cookies", "activeTab"],
  "host_permissions": ["<all_urls>"]
}